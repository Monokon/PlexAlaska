<!--Alaska-->

<includes>
	<include name="PosterMediaFlagsVars">
		<posy>530</posy>
		<width>80</width>
		<height>80</height>
		<colordiffuse>FlagDiffuse</colordiffuse>
		<aspectratio>keep</aspectratio>
	</include>
	<include name="PosterMediaFlagsAudiochannelsVars">
		<posy>530</posy>
		<width>80</width>
		<height>80</height>
		<colordiffuse>FlagDiffuseAudiochannels</colordiffuse>
		<aspectratio>keep</aspectratio>
	</include>
	<include name="PosterMediaFlags">
		<control type="group">
			<include>Animation_SlideUpFadeOut</include>
			<include>Animation_HiddenByInfo</include>
			<posx>58</posx>
			<animation effect="slide" start="0,0" end="0,20" condition="Control.IsVisible(50)">Conditional</animation>
			<animation effect="slide" start="0,0" end="0,20" condition="Control.IsVisible(51)">Conditional</animation>
			<animation effect="slide" start="0,0" end="-345,-400" condition="Control.IsVisible(52) | Control.IsVisible(552)">Conditional</animation>
			<animation effect="slide" start="0,0" end="0,35" condition="Control.IsVisible(50)">Conditional</animation>
			<animation effect="slide" start="0,0" end="-340,20" condition="Control.IsVisible(56)">Conditional</animation>
			<animation effect="slide" start="0,0" end="-340,20" condition="Control.IsVisible(56)">Conditional</animation>
			<animation effect="slide" start="0,0" end="-340,30" condition="Control.IsVisible(49)">Conditional</animation>
			<animation effect="slide" start="0,0" end="-837,-190" condition="Control.IsVisible(556) | Control.IsVisible(559)">Conditional</animation>
			<visible>Skin.HasSetting(mediaflagson)</visible>
			<!-- Year Panel -->
			<control type="group">
				<animation effect="slide" start="0,0" end="105,0" condition="IsEmpty(Listitem.Property(mediaTag::audioChannels)) + !IsEmpty(Listitem.Property(mediaTag::videoResolution)) + !IsEmpty(Listitem.Property(mediaTag::videoCodec))">Conditional</animation>
				<animation effect="slide" start="0,0" end="105,0" condition="!IsEmpty(Listitem.Property(mediaTag::audioChannels)) + IsEmpty(Listitem.Property(mediaTag::videoResolution)) + !IsEmpty(Listitem.Property(mediaTag::videoCodec))">Conditional</animation>
				<animation effect="slide" start="0,0" end="105,0" condition="!IsEmpty(Listitem.Property(mediaTag::audioChannels)) + !IsEmpty(Listitem.Property(mediaTag::videoResolution)) + IsEmpty(Listitem.Property(mediaTag::videoCodec))">Conditional</animation>
				<animation effect="slide" start="0,0" end="210,0" condition="IsEmpty(Listitem.Property(mediaTag::audioChannels)) + IsEmpty(Listitem.Property(mediaTag::videoResolution)) + !IsEmpty(Listitem.Property(mediaTag::videoCodec))">Conditional</animation>
				<animation effect="slide" start="0,0" end="210,0" condition="!IsEmpty(Listitem.Property(mediaTag::audioChannels)) + IsEmpty(Listitem.Property(mediaTag::videoResolution)) + IsEmpty(Listitem.Property(mediaTag::videoCodec))">Conditional</animation>
				<animation effect="slide" start="0,0" end="210,0" condition="IsEmpty(Listitem.Property(mediaTag::audioChannels)) + !IsEmpty(Listitem.Property(mediaTag::videoResolution)) + IsEmpty(Listitem.Property(mediaTag::videoCodec))">Conditional</animation>
				<animation effect="slide" start="0,0" end="210,0" condition="IsEmpty(Listitem.Property(mediaTag::audioChannels)) + IsEmpty(Listitem.Property(mediaTag::videoResolution)) + IsEmpty(Listitem.Property(mediaTag::videoCodec)) + !IsEmpty(Listitem.Property(mediaTag::studio))">Conditional</animation>
				<animation effect="slide" start="0,0" end="315,0" condition="IsEmpty(Listitem.Property(mediaTag::audioChannels)) + IsEmpty(Listitem.Property(mediaTag::videoResolution)) + IsEmpty(Listitem.Property(mediaTag::videoCodec)) + IsEmpty(Listitem.Property(mediaTag::studio))">Conditional</animation>
				<posx>785</posx>
				<control type="image">
					<posx>4</posx>
					<posy>554</posy>
					<width>78</width>
					<height>32</height>
					<colordiffuse>FlagDiffuse</colordiffuse>
					<texture>flags/frame_year.png</texture>
					<visible>!IsEmpty(ListItem.Year)</visible>
				</control>
				<control type="label">
					<label>$INFO[ListItem.Year]</label>
					<posx>15</posx>
					<posy>568</posy>
					<width>56</width>
					<align>center</align>
					<aligny>center</aligny>
					<font>Font_PosterRating</font>
					<textcolor>FlagDiffuse</textcolor>
					<visible>!IsEmpty(ListItem.Year)</visible>
				</control>
			</control>

			<control type="group">
				<posx>1100</posx>
				<control type="image">
					<posy>545</posy>
					<width>80</width>
					<height>50</height>
					<colordiffuse>FlagDiffuse</colordiffuse>
					<aspectratio>keep</aspectratio>
					<info>Listitem.Property(mediaTag::studio)</info>
					<visible>!IsEmpty(ListItem.Property(mediaTag::studio))</visible>
				</control>
			</control>
			<control type="group">
				<animation effect="slide" start="0,0" end="105,0" condition="IsEmpty(Listitem.Property(mediaTag::audioChannels)) + !IsEmpty(Listitem.Property(mediaTag::videoResolution))">Conditional</animation>
				<animation effect="slide" start="0,0" end="105,0" condition="!IsEmpty(Listitem.Property(mediaTag::audioChannels)) + IsEmpty(Listitem.Property(mediaTag::videoResolution))">Conditional</animation>
				<animation effect="slide" start="0,0" end="210,0" condition="IsEmpty(Listitem.Property(mediaTag::audioChannels)) + IsEmpty(Listitem.Property(mediaTag::videoResolution))">Conditional</animation>
				<posx>890</posx>
				<control type="image">
					<include>PosterMediaFlagsVars</include>
					<info>Listitem.Property(mediaTag::videoCodec)</info>
				</control>
			</control>
			<control type="group">
				<animation effect="slide" start="0,0" end="105,0" condition="IsEmpty(Listitem.Property(mediaTag::audioChannels))">Conditional</animation>
				<posx>995</posx>
				<control type="image">
					<include>PosterMediaFlagsVars</include>
					<visible>!IsEmpty(ListItem.Property(mediaType::movie))</visible>
					<info>Listitem.Property(mediaTag::videoResolution)</info>
				</control>
			</control>
			<control type="group">
				<animation effect="slide" start="0,0" end="-105,0" condition="!IsEmpty(Listitem.Property(mediaTag::studio))">Conditional</animation>
				<posx>1100</posx>
				<control type="image">
					<include>PosterMediaFlagsVars</include>
					<info>Listitem.Property(mediaTag::audioCodec)</info>
				</control>
			</control>
			<control type="group">
				<animation effect="slide" start="0,0" end="-105,0" condition="!IsEmpty(Listitem.Property(mediaTag::studio))">Conditional</animation>
				<posx>1100</posx>
				<control type="image">
					<include>PosterMediaFlagsAudiochannelsVars</include>
					<info>Listitem.Property(mediaTag::audioChannels)</info>
				</control>
			</control>

		</control>
	</include>
</includes>